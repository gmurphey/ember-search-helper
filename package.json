{
  "name": "ember-search-helper",
  "version": "1.0.5",
  "description": "Provides a helper to filter an array by a search string.",
  "keywords": [
    "ember-addon",
    "search",
    "helper",
    "filter",
    "array"
  ],
  "license": "MIT",
  "author": "Garrett Murphey <gmurphey@gmurphey.com>",
  "directories": {
    "test": "tests"
  },
  "repository": "https://github.com/gmurphey/ember-search-helper",
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint ./*.js addon addon-test-support app blueprints config lib server test-support tests",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "preversion": "rm -rf node_modules; npm install; npm test",
    "postversion": "npm publish",
    "postpublish": "git push origin $(git branch | grep '*' | sed 's/* //'); git push origin --tags; ember deploy production"
  },
  "dependencies": {
    "ember-cli-babel": "^6.6.0"
  },
  "devDependencies": {
    "bootstrap-sass": "^3.3.7",
    "broccoli-asset-rev": "^2.4.5",
    "ember-bootstrap": "^1.2.2",
    "ember-browserify": "^1.2.0",
    "ember-cli": "~3.1.4",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-deploy": "^1.0.1",
    "ember-cli-deploy-build": "^1.1.0",
    "ember-cli-deploy-git": "^1.3.0",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.1.1",
    "ember-cli-sass": "^7.0.0",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-power-select": "^2.0.2",
    "ember-resolver": "^4.0.0",
    "ember-source": "~3.1.0",
    "ember-source-channel-url": "^1.0.1",
    "ember-try": "1.0.0-beta.3",
    "eslint-plugin-ember": "^5.0.0",
    "eslint-plugin-node": "^7.0.0",
    "faker": "^4.1.0",
    "loader.js": "^4.2.3"
  },
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://gmurphey.github.io/ember-search-helper/"
  }
